{
    "root": false,
    "env": {
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "no-param-reassign": "off",
      "import/prefer-default-export": "off"
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            ["shared", "./shared"]
          ]
        }
      }
    }
  }
  